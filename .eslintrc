{
  "extends": "airbnb-base",
  "parser": "babel-eslint",
  "rules": {
    "arrow-body-style": ["error", "as-needed"],
    "comma-dangle": 0,
    "consistent-return": 0,
    "no-shadow": 0,
    "no-multi-spaces": ["error", {
      "exceptions": {
        "VariableDeclarator": true
      }
    }],
    "one-var": [2, "always"]
  }
}
